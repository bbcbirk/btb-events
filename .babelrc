{
	"presets": [
		[
			"@babel/preset-env",
			{
				"debug": false,
				"useBuiltIns": "usage",
				"corejs": {
				"version": 3,
				"proposals": true
				}
			}
		]
	],
	"plugins": [
		[
			"@babel/plugin-transform-react-jsx"
		],
		[
			"@babel/plugin-transform-runtime"
		],
		[
			"@babel/plugin-proposal-class-static-block"
		]
	]
}

